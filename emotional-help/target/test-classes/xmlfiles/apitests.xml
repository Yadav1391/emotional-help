<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Spotify  API Automation Tests " parallel="tests" thread-count="3">
    <listeners>
        <listener class-name="com.epam.reportportal.testng.ReportPortalTestNGListener"></listener>
    </listeners>
    <test name="API Tests">
        <classes>
            <class name="apitests.PostRequest"></class>
            <class name="apitests.EditPlaylist"></class>
            <class name="apitests.PlaylistTrack"></class>
            <class name="apitests.RemoveSong"></class>
        </classes>
    </test>
</suite>